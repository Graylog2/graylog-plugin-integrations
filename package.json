{
  "name": "graylog-plugin-integrations",
  "version": "4.0.6-SNAPSHOT",
  "description": "",
  "license": "SSPL-1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Graylog2/graylog-plugin-integrations.git"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint src",
    "lint:path": "eslint",
    "test": "jest",
    "flow-check": "flow check"
  },
  "eslintConfig": {
    "extends": "graylog"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/web/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "../graylog2-server/graylog2-web-interface/test/mock-FetchProvider.js",
      "../graylog2-server/graylog2-web-interface/test/console-warnings-fail-tests.js"
    ],
    "testPathIgnorePatterns": [
      ".fixtures.js$"
    ],
    "moduleDirectories": [
      "src",
      "test",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^file-loader!(.+)$": "$1",
      "\\.(css|less)$": "identity-obj-proxy",
      "^aws/(.+)$": "web/aws/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/web/fileMock.js",
      "^styled-components$": "<rootDir>/node_modules/styled-components"
    },
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "keywords": [
    "graylog"
  ],
  "author": "Graylog, Inc. <hello@graylog.com>",
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-flow": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@testing-library/dom": "7.20.2",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "^12.6.3",
    "babel-jest": "25.4.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-styled-components": "1.11.1",
    "graylog-web-plugin": "file:../graylog2-server/graylog2-web-interface/packages/graylog-web-plugin",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "react-select-event": "5.0.0"
  },
  "private": true
}
